{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "buildCommand": "bash ./scripts/vercel-prebuild.sh && bun install && cd apps/sim && bun run build",
    "installCommand": "bun install",
    "framework": null,
    "outputDirectory": "apps/sim/.next",
    "functions": {
        "apps/sim/app/api/**/*.ts": {
            "maxDuration": 300
        }
    }
}